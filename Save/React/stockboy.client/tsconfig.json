{
	"compilerOptions": {
		"baseUrl": "src",
		"paths": {
			"src/*": ["./src/*"]
		},
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"jsx": "preserve",
		"moduleResolution": "node",
		"noImplicitAny": false,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"strictNullChecks": false,
		"target": "es2023",
		"typeRoots": [ "Globals" ]
	}
}
