@using Stockboy.Models

@model TableModel
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

@{
	Layout = "~/Layouts/Main.cshtml";
	ViewData ["Title"] = "Purchases";
}

<div id="purchases_table" class="with-headspace" endpoint="UpdatePurchases">
